    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:name=".widget.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pda.pda_android.activity.welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pda.pda_android.activity.MainActivity"/>
        <activity android:name="com.pda.pda_android.activity.home.MenuManageActivity"/>
        <activity android:name="com.pda.pda_android.activity.me.ChangeBingQuActivity"/>
        <activity android:name="com.pda.pda_android.activity.me.ChangePasswordActivity"/>
        <activity android:name="com.pda.pda_android.db.testdb.DbActivity"/>
        <service
            android:name="com.pda.pda_android.service.RemindService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>